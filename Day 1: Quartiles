#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int Q1, Q2, Q3;
    int y, z, q, e, r, a, x;
    cin >> x;
    
    vector<int> W(x);
    
    for(int i = 0; i < x; i++){
        cin >> W[i];
    }
    sort(W.begin(), W.end());
    
    //when count is odd
    if(x % 2 != 0){
        z =(x/2);
        Q2 = W[z];
        a = z/2;
        e = a -1;
        Q1 = (W[a] + W[e])/2;
        cout << Q1 << endl;  
        cout << Q2 <<endl;
        q=(x-z)+a;
        r=q-1;
        Q3 = (W[q] + W[r])/2;
        cout << Q3 << endl;
    }
    else{
        y =(x/2) - 1;
        z = x/2;
        Q2 = (W[z] + W[y])/2;
       // cout << Q2 <<endl;
        q = y/2;
        Q1 = W[q];
        cout << Q1 <<endl;
        cout << Q2 <<endl;
        r = x/2 + x/4;
        Q3 = W[r];
        cout << Q3 <<endl;
    }
      
    return 0;
}
